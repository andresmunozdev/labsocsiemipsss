        "jsCode": "// DEDUPLICADOR DE ALERTAS WAZUH - Tiempo Real\nconst items = $input.all();\nconst uniqueItems = [];\nconst currentBatchIds = new Set();\n\nconsole.log(`ðŸ“¨ Procesando ${items.length} items en este batch`);\n\nfor (let i = 0; i < items.length; i++) {\n  const item = items[i];\n  \n  try {\n    // Obtener el ID Ãºnico de Wazuh desde body\n    const wazuhId = item?.body?.id;\n    \n    if (wazuhId) {\n      console.log(`ðŸ“‹ Procesando Wazuh ID: ${wazuhId}`);\n      \n      // Verificar si ya procesamos este ID en este batch\n      if (!currentBatchIds.has(wazuhId)) {\n        currentBatchIds.add(wazuhId);\n        \n        // Agregar metadata\n        item.body.n8n_wazuh_id = wazuhId;\n        item.body.n8n_is_unique = true;\n        item.body.n8n_processed_at = new Date().toISOString();\n        item.body.n8n_dedupe_method = 'batch_dedupe';\n        \n        uniqueItems.push(item);\n        \n        const ruleId = item?.body?.rule?.id || 'unknown';\n        const agentName = item?.body?.agent?.name || 'unknown';\n        const timestamp = item?.body?.timestamp || 'unknown';\n        \n        console.log(`âœ… ÃšNICA - ID: ${wazuhId}, Rule: ${ruleId}, Agent: ${agentName}, Time: ${timestamp}`);\n        \n      } else {\n        console.log(`âŒ DUPLICADA en este batch - ID: ${wazuhId}`);\n      }\n      \n    } else {\n      console.log(`âš ï¸ Item sin ID vÃ¡lido`);\n      uniqueItems.push(item);\n    }\n    \n  } catch (error) {\n    console.log(`ðŸš¨ Error: ${error.message}`);\n    uniqueItems.push(item);\n  }\n}\n\nconsole.log(`\\nðŸ“ˆ RESUMEN:`);\nconsole.log(`ðŸ“Š Items recibidos: ${items.length}`);\nconsole.log(`âœ… Items Ãºnicos: ${uniqueItems.length}`);\nconsole.log(`âŒ Duplicados en batch: ${items.length - uniqueItems.length}`);\n\nreturn uniqueItems;"
      },
